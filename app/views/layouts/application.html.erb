<!DOCTYPE html>
<html>
  <head>
    <title>Oltwitter</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="wrapper">
      <%= image_tag 'twitter-logo.png', height: '58px' %>
      <%= yield %>
    </div>
  </body>
</html>
